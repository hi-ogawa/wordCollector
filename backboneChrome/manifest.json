{
    "manifest_version": 2,

    "name": "test backbone.js in chrome extension",
    "description": "",

    "version": "1.0",

    "icons": { "16":   "icons/16.png",
	       "48":   "icons/48.png",
	       "128": "icons/128.png"
    },
    "browser_action": {
	"default_icon": "icons/16.png",
	"default_popup": "popup/popup.html"
    },
    "options_ui": {
	"page": "options/options.html",
	"chrome_style": true
    },
    "background": {
    	"scripts": [
	    "lib/jquery.js",
	    "lib/underscore.js", 
	    "contentScript/backbone/mylib.js",
	    "eventPage/eventPage.js"],
    	"persistent": false
    },
    "web_accessible_resources": [
	"contentScript/contentScript.html"
	// "contentScript/backbone/sampleAPI/dictionary0.json",
	// "contentScript/backbone/sampleAPI/dictionary1.json",
	// "contentScript/backbone/sampleAPI/categories.json"
    ],
    "permissions": [
	"storage",
	"tabs",
	"<all_urls>"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
    // content scripts can be inserted programically, see popup/popup.js
}
